<p id="notice"><%= notice %></p>

<h1>Routes</h1>

<table>
  <thead>
    <tr>
      <th>Start at</th>
      <th>Ends at</th>
      <th>Travel time</th>
      <th>Total stops</th>
      <th>Action</th>
      <th>Driver</th>
      <th>Vehicle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @routes.each do |route| %>
      <tr>
        <td><%= route.start_at %></td>
        <td><%= route.ends_at %></td>
        <td><%= route.travel_time %></td>
        <td><%= route.total_stops %></td>
        <td><%= route.status %></td>
        <td><%= route.driver_id %></td>
        <td><%= route.vehicle_id %></td>
        <td><%= link_to 'Show', route %></td>
        <td><%= link_to 'Edit', edit_route_path(route) %></td>
        <td><%= link_to 'Destroy', route, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Route', new_route_path %>
